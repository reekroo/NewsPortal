services:
  bankservice:
    build:
      context: C:\GIT - My Projects\NewsPortal
      dockerfile: BankService/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: BankService
      target: base
    container_name: BankService
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: bankservice:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "bin/Debug/netcoreapp3.1/BankService.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 52598
      target: 80
    - published: 44381
      target: 443
    tty: true
    volumes:
    - C:\GIT - My Projects\NewsPortal\BankService:/app:rw
    - C:\Users\Pavel_Davidouski\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\Pavel_Davidouski\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Pavel_Davidouski\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Microsoft\Xamarin\NuGet:/root/.nuget/fallbackpackages:ro
    - C:\Users\Pavel_Davidouski\.nuget\packages:/root/.nuget/packages:ro
